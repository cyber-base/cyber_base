{% extends 'base.html.twig' %}

{% block title %}Atelier{% endblock %}

{% block body %}
    <h1>Atelier</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ atelier.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ atelier.libelle }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ atelier.date ? atelier.date|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>HeureDebut</th>
                <td>{{ atelier.heureDebut}}</td>
            </tr>
            <tr>
                <th>HeureFin</th>
                <td>{{ atelier.heureFin}}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{{ atelier.statut }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ atelier.image }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_listeAtelier_index') }}">back to list</a>

    <a href="{{ path('app_atelier_edit', {'id': atelier.id}) }}">edit</a>

    {{ include('atelier/_delete_form.html.twig') }}
{% endblock %}
